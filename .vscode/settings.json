{
  "cSpell.words": [
    "authtoken",
    "conda",
    "Consolas",
    "createsuperuser",
    "dataframe",
    "djangorestframework",
    "dtype",
    "embedgooglemap",
    "geopy",
    "gmap",
    "Hartstonge",
    "imread",
    "imshow",
    "imwrite",
    "LBUTTONDOWN",
    "makemigrations",
    "mapouter",
    "matplotlib",
    "ndarray",
    "perfectparking",
    "pyplot",
    "Rhys",
    "runserver",
    "sqlmigrate",
    "startapp",
    "startproject",
    "viewsets"
